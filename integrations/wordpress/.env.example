# ThreadKit WordPress Plugin Configuration
# Copy this file to .env and fill in your values

# ThreadKit API Credentials
THREADKIT_SITE_ID=your-site-id-here
THREADKIT_API_KEY=your-api-key-here

# WordPress Database (for docker-compose)
WORDPRESS_DB_NAME=wordpress
WORDPRESS_DB_USER=wordpress
WORDPRESS_DB_PASSWORD=wordpress

# MySQL Root Password
MYSQL_ROOT_PASSWORD=rootpassword
