{
  "name": "@threadkit/plugin-auth-ethereum",
  "version": "0.2.0",
  "description": "Ethereum wallet authentication plugin for ThreadKit",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.tsx --format esm --dts --external react --external react-dom --external @threadkit/react --external wagmi --external viem --external @tanstack/react-query",
    "dev": "tsup src/index.tsx --format esm --dts --external react --external react-dom --external @threadkit/react --external wagmi --external viem --external @tanstack/react-query --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix"
  },
  "peerDependencies": {
    "@threadkit/react": "workspace:*",
    "react": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "wagmi": "^3.1.0",
    "viem": "^2.21.0",
    "@tanstack/react-query": "^5.59.0"
  },
  "devDependencies": {
    "@testing-library/react": "^16.0.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.15",
    "jsdom": "^27.3.0",
    "react": "^19.2.2",
    "react-dom": "^19.2.2",
    "tsup": "^8.3.5",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  },
  "license": "MIT",
  "author": "fantauma",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/usethreadkit/threadkit.git"
  },
  "bugs": {
    "url": "https://github.com/usethreadkit/threadkit/issues"
  },
  "homepage": "https://usethreadkit.com"
}
