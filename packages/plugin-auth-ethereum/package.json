{
  "name": "@threadkit/plugin-auth-ethereum",
  "version": "0.0.2",
  "description": "Ethereum wallet authentication plugin for ThreadKit",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.tsx --format esm --dts --external react --external @threadkit/react --external wagmi --external viem --external @tanstack/react-query",
    "dev": "tsup src/index.tsx --format esm --dts --external react --external @threadkit/react --external wagmi --external viem --external @tanstack/react-query --watch",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "peerDependencies": {
    "@threadkit/react": ">=0.0.1",
    "react": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "wagmi": "^2.14.0",
    "viem": "^2.21.0",
    "@tanstack/react-query": "^5.59.0"
  },
  "devDependencies": {
    "@testing-library/react": "^16.0.0",
    "@types/react": "^18.3.12",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.9",
    "jsdom": "^25.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "vitest": "^2.1.9"
  },
  "license": "MIT",
  "author": "fantauma",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/usethreadkit/threadkit.git"
  },
  "bugs": {
    "url": "https://github.com/usethreadkit/threadkit/issues"
  },
  "homepage": "https://usethreadkit.com"
}
