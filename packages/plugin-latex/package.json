{
  "name": "@threadkit/plugin-latex",
  "version": "0.2.0",
  "description": "LaTeX/math rendering plugin for ThreadKit using KaTeX",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --external @threadkit/core",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix"
  },
  "dependencies": {
    "@threadkit/core": "workspace:*"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.15",
    "tsup": "^8.3.5",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  },
  "license": "MIT",
  "author": "fantauma",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/usethreadkit/threadkit.git"
  },
  "bugs": {
    "url": "https://github.com/usethreadkit/threadkit/issues"
  },
  "homepage": "https://usethreadkit.com"
}
